<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite thread-count="10" verbose="1" name="Gmail Suite"
	annotations="JDK" parallel="tests">
	<listeners>
		<listener
			class-name="selenium.ui.framework.listeners.TestListener" />
		<listener
			class-name="selenium.ui.framework.utils.TestListSort" />
	</listeners>
	<test name="GoogleTest">
		<classes>
			<class name="sample.tests.kickoff.google.GoogleMainTest" />
		</classes>
	</test>


</suite>